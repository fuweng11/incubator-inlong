                <ThresholdFilter level="warn" onMatch="DENY" onMismatch="NEUTRAL"/>
                <ThresholdFilter level="info" onMatch="ACCEPT" onMismatch="DENY"/>
            </Filters>
            <PatternLayout pattern="[%d{yyyy-MM-dd HH:mm:ss:SSS}][%p]-[%C{1}:%L]-%m%n"/>
            <Policies>
                <SizeBasedTriggeringPolicy size="1GB"/>
            </Policies>
            <DefaultRolloverStrategy max="15"/>
        </RollingFile>
        <RollingFile name="RollingJobManagerReport" fileName="${basePath}/monitor/${log.file.name.job.manager.report.prefix}${log.file.info.name.postfix}"
                     filePattern="${basePath}/monitor/${log.file.name.job.manager.report.prefix}${log.file.info.name.postfix}.%i">
            <!--控制台只输出level及以上级别的信息（onMatch），其他的直接拒绝（onMismatch）-->
            <Filters>
                <ThresholdFilter level="error" onMatch="DENY" onMismatch="NEUTRAL"/>
                <ThresholdFilter level="warn" onMatch="DENY" onMismatch="NEUTRAL"/>
                <ThresholdFilter level="info" onMatch="ACCEPT" onMismatch="DENY"/>
            </Filters>
            <PatternLayout pattern="[%d{yyyy-MM-dd HH:mm:ss:SSS}][%p]-[%C{1}:%L]-%m%n"/>
            <Policies>
                <SizeBasedTriggeringPolicy size="1GB"/>
            </Policies>
            <DefaultRolloverStrategy max="15"/>
        </RollingFile>
        <RollingFile name="RollingJobSenderReport" fileName="${basePath}/monitor/${log.file.name.job.sender.report.prefix}${log.file.info.name.postfix}"
                     filePattern="${basePath}/monitor/${log.file.name.job.sender.report.prefix}${log.file.info.name.postfix}.%i">
            <!--控制台只输出level及以上级别的信息（onMatch），其他的直接拒绝（onMismatch）-->
            <Filters>
                <ThresholdFilter level="error" onMatch="DENY" onMismatch="NEUTRAL"/>
                <ThresholdFilter level="warn" onMatch="DENY" onMismatch="NEUTRAL"/>
                <ThresholdFilter level="info" onMatch="ACCEPT" onMismatch="DENY"/>
            </Filters>
            <PatternLayout pattern="[%d{yyyy-MM-dd HH:mm:ss:SSS}][%p]-[%C{1}:%L]-%m%n"/>
            <Policies>
                <SizeBasedTriggeringPolicy size="1GB"/>
            </Policies>
            <DefaultRolloverStrategy max="15"/>
        </RollingFile>
        <RollingFile name="RollingPulsar" fileName="${basePath}/monitor/${log.file.name.pulsar.prefix}${log.file.info.name.postfix}"
                     filePattern="${basePath}/monitor/${log.file.name.pulsar.prefix}${log.file.info.name.postfix}.%i">
            <!--控制台只输出level及以上级别的信息（onMatch），其他的直接拒绝（onMismatch）-->
            <Filters>
                <ThresholdFilter level="error" onMatch="DENY" onMismatch="NEUTRAL"/>
                <ThresholdFilter level="warn" onMatch="DENY" onMismatch="NEUTRAL"/>
                <ThresholdFilter level="info" onMatch="ACCEPT" onMismatch="DENY"/>
            </Filters>
            <PatternLayout pattern="[%d{yyyy-MM-dd HH:mm:ss:SSS}][%p]-[%C{1}:%L]-%m%n"/>
            <Policies>
                <SizeBasedTriggeringPolicy size="1GB"/>
            </Policies>
            <DefaultRolloverStrategy max="15"/>
        </RollingFile>
        <RollingFile name="RollingSendMetric" fileName="${basePath}/monitor/${log.file.name.job.sender.metric.prefix}${log.file.info.name.postfix}"
                     filePattern="${basePath}/monitor/${log.file.name.job.sender.metric.prefix}${log.file.info.name.postfix}.%i">
            <!--控制台只输出level及以上级别的信息（onMatch），其他的直接拒绝（onMismatch）-->
            <Filters>
                <ThresholdFilter level="error" onMatch="DENY" onMismatch="NEUTRAL"/>
                <ThresholdFilter level="warn" onMatch="DENY" onMismatch="NEUTRAL"/>
                <ThresholdFilter level="info" onMatch="ACCEPT" onMismatch="DENY"/>
            </Filters>
            <PatternLayout pattern="[%d{yyyy-MM-dd HH:mm:ss:SSS}][%p]-[%C{1}:%L]-%m%n"/>
            <Policies>
                <SizeBasedTriggeringPolicy size="1GB"/>
            </Policies>
            <DefaultRolloverStrategy max="15"/>
        </RollingFile>
        <RollingFile name="RollingAlterFieldMonitorInfo" fileName="${basePath}/monitor/${log.file.name.alter.field.prefix}${log.file.info.name.postfix}"
                     filePattern="${basePath}/monitor/${log.file.name.alter.field.prefix}${log.file.info.name.postfix}.%i">
            <!--控制台只输出level及以上级别的信息（onMatch），其他的直接拒绝（onMismatch）-->
            <Filters>
                <ThresholdFilter level="error" onMatch="DENY" onMismatch="NEUTRAL"/>
                <ThresholdFilter level="warn" onMatch="DENY" onMismatch="NEUTRAL"/>
                <ThresholdFilter level="info" onMatch="ACCEPT" onMismatch="DENY"/>
            </Filters>
            <PatternLayout pattern="[%d{yyyy-MM-dd HH:mm:ss:SSS}][%p]-[%C{1}:%L]-%m%n"/>
            <Policies>
                <SizeBasedTriggeringPolicy size="1GB"/>
            </Policies>
            <DefaultRolloverStrategy max="15"/>
        </RollingFile>
        <RollingFile name="RollingJobCoordinatorInfo" fileName="${basePath}/monitor/${log.file.name.job.coordinator.prefix}${log.file.info.name.postfix}"
                     filePattern="${basePath}/monitor/${log.file.name.job.coordinator.prefix}${log.file.info.name.postfix}.%i">
            <!--控制台只输出level及以上级别的信息（onMatch），其他的直接拒绝（onMismatch）-->
