            </Filters>
            <PatternLayout pattern="[%d{yyyy-MM-dd HH:mm:ss:SSS}][%p]-[%C{1}:%L]-%m%n"/>
            <Policies>
                <SizeBasedTriggeringPolicy size="1GB"/>
            </Policies>
            <DefaultRolloverStrategy max="15"/>
        </RollingFile>
    </appenders>

    <loggers>
        <!--默认不打印-->
        <logger name="org.apache.pulsar" level="error" additivity="false">
            <appender-ref ref="RollingPulsar"/>
        </logger>
        <logger name="eventDiscardMonitor" level="info" additivity="false">
            <appender-ref ref="RollingEventDiscardMonitorInfo"/>
        </logger>
        <logger name="jobBigFieldMonitor" level="info" additivity="false">
            <appender-ref ref="RollingBigFieldMonitorInfo"/>
        </logger>
        <!--显示的当前链接的db ip-port信息-->
        <logger name="jobRunStatMonitor" level="info" additivity="false">
            <appender-ref ref="RollingJobRunStatMonitorInfo"/>
        </logger>
        <logger name="startPositionChangedMonitor" level="info" additivity="false">
            <appender-ref ref="RollingStartPositionChangedMonitorInfo"/>
        </logger>
        <logger name="jobAlterFieldMonitor" level="info" additivity="false">
            <appender-ref ref="RollingAlterFieldMonitorInfo"/>
        </logger>
        <logger name="jobReport" level="info" additivity="false">
            <appender-ref ref="RollingJobReport"/>
        </logger>
        <logger name="jobManagerReport" level="info" additivity="false">
            <appender-ref ref="RollingJobManagerReport"/>
        </logger>
        <logger name="jobSenderReport" level="info" additivity="false">
            <appender-ref ref="RollingJobSenderReport"/>
        </logger>
        <!--数据量太大，默认不打印-->
        <logger name="jobSendMetric" level="error" additivity="false">
            <appender-ref ref="RollingSendMetric"/>
        </logger>
        <logger name="jobCoordinator" level="info" additivity="false">
            <appender-ref ref="RollingJobCoordinatorInfo"/>
        </logger>
        <logger name="jobTaskMonitor" level="info" additivity="false">
            <appender-ref ref="RollingJobTaskInfo"/>
        </logger>
        <logger name="jobDumpMonitor" level="info" additivity="false">
            <appender-ref ref="RollingJobDumpInfo"/>
        </logger>
        <logger name="jobZkPositionMonitor" level="info" additivity="false">
            <appender-ref ref="RollingJobZkPositionInfo"/>
        </logger>
        <root level="${output_log_level}">
            <appender-ref ref="Console"/>
            <appender-ref ref="DebugFile"/>
            <appender-ref ref="InfoFile"/>
            <appender-ref ref="WarnFile"/>
            <appender-ref ref="ErrorFile"/>
        </root>
    </loggers>
</configuration>