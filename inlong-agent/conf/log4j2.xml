        <RollingFile name="RollingJobZkPositionInfo" fileName="${basePath}/monitor/${log.file.name.job.update.zk.prefix}${log.file.info.name.postfix}"
                     filePattern="${basePath}/monitor/${log.file.name.job.update.zk.prefix}${log.file.info.name.postfix}.%i">
            <!--控制台只输出level及以上级别的信息（onMatch），其他的直接拒绝（onMismatch）-->
