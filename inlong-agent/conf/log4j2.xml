                <ThresholdFilter level="warn" onMatch="DENY" onMismatch="NEUTRAL"/>
                <ThresholdFilter level="info" onMatch="ACCEPT" onMismatch="DENY"/>
            </Filters>
            <PatternLayout pattern="[%d{yyyy-MM-dd HH:mm:ss:SSS}][%p]-[%C{1}:%L]-%m%n"/>
            <Policies>
                <SizeBasedTriggeringPolicy size="1GB"/>
            </Policies>
            <DefaultRolloverStrategy max="15"/>
        </RollingFile>
        <RollingFile name="RollingJobReport" fileName="${basePath}/monitor/${log.file.name.job.report.prefix}${log.file.info.name.postfix}"
                     filePattern="${basePath}/monitor/${log.file.name.job.report.prefix}${log.file.info.name.postfix}.%i">
            <!--控制台只输出level及以上级别的信息（onMatch），其他的直接拒绝（onMismatch）-->
            <Filters>
